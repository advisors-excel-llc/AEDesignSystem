  },
  "homepage": "https://github.com/advisors-excel-llc/AEDesignSystem#readme",
  "dependencies": {
    "@salesforce-ux/design-system": "^2.7.4",
    "@salesforce/design-system-react": "^0.8.29",
    "formik": "^1.5.4",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
